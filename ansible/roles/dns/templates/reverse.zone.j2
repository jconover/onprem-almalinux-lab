; Managed by Ansible -- do not edit
$TTL 86400
@   IN  SOA ns1.{{ dns_domain }}. admin.{{ dns_domain }}. (
            2024010101  ; Serial
            3600        ; Refresh
            1800        ; Retry
            604800      ; Expire
            86400 )     ; Minimum TTL

    IN  NS  ns1.{{ dns_domain }}.

{% for record in dns_records %}
{{ record.ip.split('.')[-1] }}  IN  PTR  {{ record.name }}.{{ dns_domain }}.
{% endfor %}
